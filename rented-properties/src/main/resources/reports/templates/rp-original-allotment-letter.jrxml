				<textElement>
					<font size="11"/>
				</textElement>
				<textFieldExpression><![CDATA[$P{Owner}.getOwnerDetails().getName()+"\n"+$P{colony}+"\n"+$F{PropertyDetails}.getAddress().getArea()+", "+$F{PropertyDetails}.getAddress().getPincode()+"\n"+"Memo no/Allotment number : "+$P{Owner}.getAllotmenNumber()]]></textFieldExpression>
